<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle"
    android:layout_height="match_parent" android:layout_width="match_parent">

    <gradient
        android:angle="90"
        android:startColor="#346A17"
        android:endColor="#1B431B"

        />


    <padding
        android:bottom="7dp"
        android:left="7dp"
        android:right="7dp"
        android:top="7dp"
        />

    <stroke
        android:width="1dp"
        android:color="#39853C"
        />

    <corners
        android:radius="20dp"

        />



</shape>